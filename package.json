{
  "name": "vaultaire",
  "version": "0.10.0",
  "description": "JavaScript/Typescript client for HashiCorp's Vault",
  "main": "./src/index.js",
  "type": "module",
  "engines": {
    "node": ">=12",
    "pnpm": "^6"
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "docker compose up -d vault",
    "test": "VAULT_ADDR=http://localhost:8200 nyc mocha",
    "posttest": "nyc report --reporter=json",
    "test:unit": "mocha test/unit.js",
    "test:watch": "mocha --watch",
    "docs": "node scripts/write-features.js && docco --output docs src/*.js",
    "precommit": "node scripts/write-features.js && git add features.md"
  },
  "dependencies": {
    "axios": "0.26.1",
    "debug": "4.3.4",
    "mustache": "4.2.0",
    "tv4": "1.3.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "chai": "4.3.6",
    "dirty-chai": "2.0.1",
    "docco": "0.9.1",
    "eslint": "8.14.0",
    "husky": "7.0.4",
    "mocha": "9.2.2",
    "nyc": "15.1.0",
    "sinon": "13.0.2",
    "sinon-chai": "3.7.0",
    "typescript": "^4.6.3"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/vaultaire/vaultaire.git"
  },
  "directories": {
    "example": "./example",
    "doc": "./docs"
  },
  "files": [
    "src",
    "index.d.ts"
  ],
  "keywords": [
    "vault",
    "hashicorp",
    "secrets",
    "manage",
    "client"
  ],
  "author": "kr1sp1n <krispin@posteo.de> (https://github.com/kr1sp1n)",
