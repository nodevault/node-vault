{
  "name": "vaultaire",
  "version": "0.10.0",
  "description": "JavaScript/Typescript client for HashiCorp's Vault",
  "main": "./src/index.js",
  "engines": {
    "pnpm": "^6"
  },
  "scripts": {
    "precoverage": "pnpm run lint",
    "lint": "eslint .",
    "pretest": "docker compose up -d vault",
    "test": "VAULT_ADDR=http://localhost:8200 nyc mocha",
    "posttest": "nyc report --reporter=json",
    "test:unit": "mocha test/unit.js",
    "test:watch": "mocha --watch",
    "predocs": "node scripts/write-features.js",
    "docs": "docco --output docs src/*.js",
    "precommit": "node scripts/write-features.js && git add features.md"
  },
  "directories": {
    "example": "./example",
    "doc": "./docs"
  },
  "files": [
    "src",
    "index.d.ts"
  ],
  "dependencies": {
    "debug": "3.1.0",
    "mustache": "^2.2.1",
    "request": "2.88.2",
    "request-promise-native": "1.0.7",
    "tv4": "^1.2.7"
  },
  "devDependencies": {
    "@types/request": "2.48.8",
    "chai": "3.5.0",
    "codecov": "3.8.3",
    "dirty-chai": "1.2.2",
    "docco": "0.8.0",
    "eslint": "2.13.1",
    "eslint-config-airbnb": "9.0.1",
    "eslint-plugin-import": "1.16.0",
    "eslint-plugin-jsx-a11y": "1.2.0",
    "eslint-plugin-react": "5.2.2",
    "husky": "0.14.3",
    "mocha": "9.2.2",
    "nyc": "15.1.0",
    "sinon": "1.17.3",
    "sinon-chai": "2.14.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/vaultaire/vaultaire.git"
  },
  "keywords": [
    "vault",
    "hashicorp",
    "secrets",
    "manage",
    "client"
  ],
  "author": "kr1sp1n <krispin@posteo.de> (https://github.com/kr1sp1n)",
