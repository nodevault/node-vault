{
  "extends": "airbnb",
  "parserOptions": {
    "sourceType": "script"
  },
  "rules": {
    "strict": 0,
    "no-param-reassign": [
      "error",
      {
        "props": false
      }
    ],
    "no-console": 0,
    "jsx-a11y/href-no-hash": "off"
  },
  "globals": {
    "describe": true,
    "it": true,
    "after": true,
    "afterEach": true,
    "before": true,
    "beforeEach": true,
    "should": true,
    "require": true,
    "process": true
  },
  "env": {
    "node": true,
    "mocha": true
  }
}
